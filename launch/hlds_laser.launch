    <param name="port" type="string" value="/dev/ttyS1"/>
    <param name="frame_id_" value="laser"/>
    <param name="laserGen" type="int" value="2"/>
    <param name="block_angle_1_" type="int" value="8"/>
    <param name="block_angle_2_" type="int" value="127"/>
    <param name="block_angle_3_" type="int" value="249"/>
    <param name="block_range_" type="int" value="33"/>
    <param name="LIDAR_OFFSET_X_" type="double" value="0.031"/>
    <param name="LIDAR_OFFSET_Y_" type="double" value="0"/>
    <param name="LIDAR_OFFSET_THETA_" type="double" value="205"/>
    <param name="delay_when_republish_" type="int" value="0"/>
  </node>
</launch>

